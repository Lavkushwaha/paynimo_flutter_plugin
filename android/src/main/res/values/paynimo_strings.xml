    <!-- Card Regex -->
    <string name="regex_visa">^4[0-9]*</string>
    <string name="regex_rupay">^(606([1-9])|607|608|652)</string>
    <string name="regex_mastercard">^5[1-5][0-9]*</string>
    <string name="regex_maestro">^(6220|5044|5018|5081|5020|5038|5893|6304|6331|6759|676[1-3]|6799|0604)[0-9]*</string>
    <string name="regex_amex">^3[47][0-9]*</string>
    <string name="regex_jcb">^35((28)|(29)|[3-8][0-9])[0-9]*</string>
    <string name="regex_dinersclub">^(36|38|30[0-5])[0-9]*</string>
    <string name="regex_discover">^(65|64[4-9]|6011|622(1(2[6-9]|[3-9][0-9])|[2-8][0-9]{2}|9([01][0-9]|2[0-5])))[0-9]*</string>
    <string name="regex_unionpay">^62[0-9]*</string>
    <string name="regex_instapayment">^63[7-9][0-9]*</string>
    <string name="regex_laser">^(6704|6706|6771|6709)[0-9]*</string>
    <string name="regex_ifsc">[A-Z|a-z|0-9]{11}</string>
    <string name="regex_acc_holder_name">^[a-zA-Z0-9\'. ]+$</string>
    <string name="regex_acc_no">^[a-zA-Z0-9]+$</string>
    <string name="regex_pan">^([a-zA-Z]){5}([0-9]){4}([a-zA-Z]){1}?$</string>
    <string name="regex_mobile">[0-9]{10}</string>
    <string name="regex_number_only">[0-9]+</string>

    <string name="regex_visa_partial">^4</string>
    <string name="regex_rupay_partial">^(607802)</string>
    <string name="regex_mastercard_partial">^5(?![^1-5])</string>
    <string name="regex_maestro_partial">^5(?![^08])(0?(?![^123])(1?(?![^8])|2?(?![^0])|3?(?![^8]))|(?&lt;![^5])8?(?![^9])(?&lt;![^8])9?(?![^3]))|^6((?![^3])3?((?![^0])0?(?![^4])|(?![^3])(?&lt;![^3])3?(?![^1]))|(?![^7])7((?![^5])5?(?![^9])|(?![^6])6?(?![^1-3])|(?![^9])(?&lt;![^7])9(?![^9])))|^0(?![^6])6?(?![^0])0?(?![^4])</string>
    <string name="regex_amex_partial">^3(?![^47])</string>
    <string name="regex_jcb_partial">^3(?![^5])5?((?![^2])2?(?![^89])|(?![^3-8])(?&lt;![^5])[3-8]?)</string>
    <string name="regex_dinersclub_partial">^3((?![^68])|0(?![^0-5]))</string>
    <string name="regex_discover_partial">^6((?![^5])|(?![^4])4(?![^4-9])|(?![^0])0?($|(?![^1])1(?![^1])))</string>
    <string name="regex_unionpay_partial">^6(?![^2])</string>
    <string name="regex_instapayment_partial">^6(?![^3])3?(?![^7-9])</string>
    <string name="regex_laser_partial">^6(?![^7])7?(?![^07])(0?(?![^469])|(?&lt;![^7])7?(?![^1]))</string>

